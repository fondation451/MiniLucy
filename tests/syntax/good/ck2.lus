
node ck2 (c: bool; d: bool ; e: int)
returns (n: int);  
var
  d_on_c : bool when True(c);

let
  d_on_c = d when True(c);
  n = ck2_clk(c,  d_on_c , e when True(d_on_c));
tel


node ck2_clk (c: bool; d: bool when True(c) ; e: int when True(d))
returns (n: int);
let
  n = if c and current(d) then 0 else current(current(e));
tel
